---
import BaseLayout from "@layouts/BaseLayout.astro";
import Hero from "@/components/sections/Hero.astro";
import Footer from "@/components/sections/Footer.astro";



const { pageTitle = "", acfTitle = "", acfDescription = "", } = Astro.props;


---

<BaseLayout pageTitle={pageTitle} >

  <Hero 
    titleHero={acfTitle} 
    descriptionHero={acfDescription} 
  />
  <slot />  
  <slot name="que-hacemos" />
  <slot name="quienes-somos" />
  <slot name="oportunidades-europeas"/>

  <Footer />

</BaseLayout>
